<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.clothclassfi">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_new"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_new_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">

        <activity android:name=".FinalActivity" />
        <activity android:name=".SubdivisionActivity" />
        <activity android:name=".SubdivisionBottom" />
        <activity android:name=".SubdivisionOuter"/>
        <activity
            android:name=".top_dialog.TopPlusDialogActivity"
            android:theme="@style/DetailDialog" />
        <activity
            android:name=".BottomPlusDialogActivity"
            android:theme="@style/DetailDialog"/>
        <activity android:name=".outer_dialog.OuterPlusDialogActivity"
            android:theme="@style/DetailDialog"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity> <!-- theme이 액티비티를 다이얼로그화. layout의 background에 색을 넣었더니 둥글게 안됐다.. 왜일까-->
        <activity
            android:name=".top_dialog.TopDialogDetailActivity"
            android:theme="@style/DetailDialog" />
        <activity
            android:name=".bottom_dialog.BottomDialogDetailActivity"
            android:theme="@style/DetailDialog" />
        <activity android:name=".outer_dialog.OuterDialogDetailActivity"
            android:theme="@style/DetailDialog"/>
    </application>

</manifest>